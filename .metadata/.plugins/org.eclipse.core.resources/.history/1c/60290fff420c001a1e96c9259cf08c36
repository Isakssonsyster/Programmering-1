package Vecka45;

import java.awt.datatransfer.FlavorTable;
import java.util.Scanner;

public class UppgiftV45 {
	
	public static void main(String[] args) {
		
		System.out.println(fluidPressure(FluidTable.WATER, 10));
		
	}
	
	public static double FahrenheitToCelsius(double fahrenheit) {
		
		double FahrenheitToCelsius = (fahrenheit-32)*5/(9);
		return FahrenheitToCelsius;
		
			}

	public static double kelvinToCelsius(double kelvin) {
		
		double kelvinToCelsius = kelvin-273.15;
		return kelvinToCelsius;
		
	}
	
	public static double fluidPressure(FluidTable fluid, double i) {
		
		double fluidPressure = (FluidTable.WATER.density) * i;
		
		
		return fluidPressure;
		
	}

	
}
